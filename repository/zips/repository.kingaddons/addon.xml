<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="repository.kingaddons" name="King Addons Repository" version="5.0.0" provider-name="king">
    <requires>
        <import addon="xbmc.addon" version="12.0.0"/>
    </requires>
    <extension point="xbmc.addon.repository" name="King Addons Repository">
        <dir>
		<info compressed="false"></info>
		<checksum></checksum>
		<datadir zip="true"></datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en"></summary>
        <description lang="en"></description>
        <disclaimer lang="en"></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
        <news></news>
    </extension>
</addon>
